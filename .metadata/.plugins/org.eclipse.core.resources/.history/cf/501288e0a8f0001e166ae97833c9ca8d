package ej4;

public class NumerosFibonacci {

	public String getNumeroFibonacci(int inicio, int cantidad, int primerNumero, int segundoNumero) {
		String numeros = "0 1";
		
		while(inicio < cantidad) {
			int suma = primerNumero + segundoNumero;
			numeros += " " + suma;
		}
		
		return numeros;
	}
}
